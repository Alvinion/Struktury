#include "stdafx.h"
#include <iostream>

using namespace std;


struct typy{

	int liczba;
	char znak;
	double liczba2;

};

typy** losuj(int _n){
	
	typy** tab= new typy*[_n];	

	for (short x=0;x<_n;x++)
	{
		tab[x]=new typy;
		tab[x]->liczba=rand();
		tab[x]->znak=rand();
		tab[x]->liczba2=0;
	}

	return(tab);
}

void kasuj(typy** tab, int _n)
{
	for(short a=0; a<_n;a++)
	{
		delete tab[a];
	}

	delete tab;

}

void sortuj(typy** tab, int _n){

	for(short a=0;a<_n-1;a++)
	 for(short b=0;b<_n-1;b++)
		 if(tab[b+1]<tab[b])
			 swap(tab[b+1],tab[b]);

	
}


void zlicz(typy** tab, int _n, char znak){




}

